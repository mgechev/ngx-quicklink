{
  "name": "ngx-quicklink",
  "version": "0.2.7",
  "description": "Quicklink for Angular",
  "keywords": [
    "angular",
    "prefetching",
    "router",
    "performance",
    "prefetch",
    "background",
    "speed",
    "fetch",
    "intersectionobserver"
  ],
  "main": "index.umd.js",
  "module": "index.js",
  "scripts": {
    "start": "nodemon -e ts --watch src --exec \"npm run build\"",
    "format": "prettier --single-quote --write src/*.ts",
    "build": "ng-packagr -p ./src/ng-package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgechev/ngx-quicklink.git"
  },
  "author": "Minko Gechev <mgechev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mgechev/ngx-quicklink/issues"
  },
  "homepage": "https://github.com/mgechev/ngx-quicklink#readme",
  "devDependencies": {
    "@angular/common": "13.1.3",
    "@angular/compiler": "13.1.3",
    "@angular/compiler-cli": "13.1.3",
    "@angular/core": "13.1.3",
    "@angular/platform-browser": "13.1.3",
    "@angular/router": "13.1.3",
    "ng-packagr": "13.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^1.15.3",
    "rxjs": "^7.5.2",
    "typescript": "^4.3.5"
  }
}